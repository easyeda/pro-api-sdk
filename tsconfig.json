{
	"compilerOptions": {
		"target": "ESNext",
		"jsx": "preserve",
		"baseUrl": ".",
		"module": "ESNext",
		"moduleResolution": "Bundler",
		"resolveJsonModule": true,
		"allowJs": true,
		"strict": true,
		"noUnusedLocals": true,
		"outDir": "./dist/",
		"esModuleInterop": true,
		"skipLibCheck": true
	},
	"include": ["./src/**/*", "./node_modules/@jlceda/pro-api-types/**/*"]
}
